                <Style TargetType="ToggleButton">
                    <Setter Property="IsTabStop" Value="False"/>
                    <Setter Property="Cursor" Value="Hand"/>
                    <Setter Property="BorderBrush" Value="Transparent"/>
                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                    <Setter Property="VerticalAlignment" Value="Stretch"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="ToggleButton">
                                <Grid Background="Transparent">
                                    <TextBlock x:Name="CollapsedVisual"
                                            Foreground="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"
                                            Text="{StaticResource GlyphArrow60Right}"
                                            SnapsToDevicePixels="True"
                                            FontFamily="{StaticResource TelerikWebUI}"
                                            FontSize="16"
                                            FontWeight="Normal"
                                            FontStyle="Normal"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"/>
                                    <TextBlock x:Name="ExpandedVisual"
                                            Visibility="Collapsed"
                                            SnapsToDevicePixels="True"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Text="{StaticResource GlyphArrow45DownRight}"
                                            FontSize="16"
                                            FontWeight="Normal"
                                            FontStyle="Normal"
                                            Foreground="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"
                                            FontFamily="{StaticResource TelerikWebUI}"/>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsChecked" Value="True">
                                        <Setter TargetName="ExpandedVisual" Property="Visibility" Value="Visible"/>
                                        <Setter TargetName="CollapsedVisual" Property="Visibility" Value="Collapsed"/>
                                    </Trigger>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="ExpandedVisual" Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                                        <Setter TargetName="CollapsedVisual" Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="ExpandedVisual" Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"/>
                                        <Setter TargetName="CollapsedVisual" Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"/>
                                    </Trigger>
                                    <Trigger Property="FlowDirection" Value="RightToLeft">
                                        <Setter TargetName="ExpandedVisual" Property="Text" Value="{StaticResource GlyphArrow60Left}"/>
                                        <Setter TargetName="CollapsedVisual" Property="Text" Value="{StaticResource GlyphArrow45DownLeft}"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
