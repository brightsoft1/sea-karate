                    <telerikNavigation:ToolBarContainerStyle TypeName="TextBox" ContainerStyle="{StaticResource ToolBarTextBoxStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="Button" ContainerStyle="{StaticResource ToolBarButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RepeatButton" ContainerStyle="{StaticResource ToolBarRepeatButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="ToggleButton" ContainerStyle="{StaticResource ToolBarToggleButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="CheckBox" ContainerStyle="{StaticResource ToolBarCheckBoxStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadioButton" ContainerStyle="{StaticResource ToolBarRadioButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadButton" ContainerStyle="{StaticResource ToolBarRadButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadPathButton" ContainerStyle="{StaticResource ToolBarRadPathButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadToggleButton" ContainerStyle="{StaticResource ToolBarRadToggleButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadRadioButton" ContainerStyle="{StaticResource ToolBarRadRadioButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadDropDownButton" ContainerStyle="{StaticResource ToolBarRadDropDownButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadSplitButton" ContainerStyle="{StaticResource ToolBarRadSplitButtonStyle}"/>
