                    <Grid x:Name="RootElement" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="c0" Width="Auto"/>
                            <ColumnDefinition x:Name="c1" Width="*"/>
                            <ColumnDefinition x:Name="c2" Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="r0" Height="Auto"/>
                            <RowDefinition x:Name="r1" Height="*"/>
                            <RowDefinition x:Name="r2" Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.ColumnSpan="3"
                                Grid.RowSpan="3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"/>
                        <telerik:RadToggleButton x:Name="PART_DropdownButton"
                                Grid.Column="2"
                                Grid.RowSpan="2"
                                Visibility="{TemplateBinding OverflowButtonVisibility}"
                                IsEnabled="{TemplateBinding HasOverflowItems}"
                                IsChecked="{TemplateBinding IsOverflowOpen}"
                                Style="{StaticResource ToolBarOverflowButtonStyle}">
                            <TextBlock x:Name="arrow"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    Text="{StaticResource GlyphArrowOverflowDown}"
                                    Foreground="{Binding (TextElement.Foreground), RelativeSource={RelativeSource AncestorType=ContentPresenter}}"
                                    FontSize="16"/>
                        </telerik:RadToggleButton>
                        <Grid x:Name="PopupGrid" Grid.Column="2">
                            <Popup x:Name="PART_Overflow"
                                    AllowsTransparency="True"
                                    StaysOpen="False"
                                    Focusable="false"
                                    Placement="Bottom"
                                    VerticalOffset="1"
                                    HorizontalOffset="0"
                                    IsOpen="{Binding Path=IsOverflowOpen, RelativeSource={RelativeSource TemplatedParent}}"
                                    PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">
                                <Border
                                        BorderBrush="{telerik1:Office2016Resource ResourceKey=BasicBrush}"
                                        Background="{telerik1:Office2016Resource ResourceKey=PrimaryBrush}"
                                        BorderThickness="1"
                                        MinWidth="55"
                                        MinHeight="26"
                                        CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}">
                                    <telerikToolBar:RadToolBarOverflowPanel x:Name="PART_OverflowPanel" Orientation="Vertical" VerticalAlignment="Top"/>
                                </Border>
                            </Popup>
                        </Grid>
                        <Grid Visibility="{TemplateBinding GripVisibility}" Background="Transparent">
                            <TextBlock x:Name="GripOrnamentH"
                                    Visibility="Visible"
                                    Foreground="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="16"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    Text="{StaticResource GlyphMoreVertical}"/>
                            <TextBlock x:Name="GripOrnamentV"
                                    Visibility="Collapsed"
                                    Foreground="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"
                                    VerticalAlignment="Top"
                                    HorizontalAlignment="Center"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="16"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    Text="{StaticResource GlyphMoreHorizontal}"/>
                        </Grid>
                        <telerikToolBar:RadToolBarPanel x:Name="PART_StripPanel" Grid.Column="1" Orientation="Horizontal" Margin="{TemplateBinding Padding}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Orientation" Value="Horizontal"/>
                        <Trigger Property="Orientation" Value="Vertical">
                            <Setter TargetName="c0" Property="Width" Value="*"/>
                            <Setter TargetName="c1" Property="Width" Value="Auto"/>
                            <Setter TargetName="c2" Property="Width" Value="Auto"/>
                            <Setter TargetName="PART_StripPanel" Property="Orientation" Value="Vertical"/>
                            <Setter TargetName="PART_StripPanel" Property="Grid.Row" Value="1"/>
                            <Setter TargetName="PART_StripPanel" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Row" Value="2"/>
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="GripOrnamentH" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="GripOrnamentV" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="PopupGrid" Property="Grid.Row" Value="2"/>
                            <Setter TargetName="PopupGrid" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_Overflow" Property="Placement" Value="Right"/>
                            <Setter TargetName="PART_Overflow" Property="HorizontalOffset" Value="1"/>
                            <Setter TargetName="PART_Overflow" Property="VerticalOffset" Value="0"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="GripOrnamentH" Property="Opacity" Value="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="GripOrnamentV" Property="Opacity" Value="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
