    <HierarchicalDataTemplate x:Key="StructureItemHierarchicalDataTemplate" ItemsSource="{Binding Items}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Path Data="{Binding OriginalItemType, Converter={StaticResource TypeToPathDataConverter}}" Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType=Control, AncestorLevel=2, Mode=FindAncestor}}" Margin="0 0 5 0"/>
            <TextBlock Text="{Binding Header}" Grid.Column="1"/>
        </Grid>
