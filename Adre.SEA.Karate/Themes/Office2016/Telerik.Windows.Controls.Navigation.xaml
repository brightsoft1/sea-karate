                        <Border x:Name="MaximizeAreaBottom" Grid.ColumnSpan="2" Grid.Row="1" Visibility="Collapsed" Background="{telerik1:Office2016Resource ResourceKey=AccentBrush}" Opacity="0.25"/>
                        <Border x:Name="MaximizeAreaLeft" Grid.RowSpan="2" Grid.Column="0" Visibility="Collapsed" Background="{telerik1:Office2016Resource ResourceKey=AccentBrush}" Opacity="0.25"/>
