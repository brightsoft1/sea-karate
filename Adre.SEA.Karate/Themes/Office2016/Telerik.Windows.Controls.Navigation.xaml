                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <Grid x:Name="markGrid" Background="Transparent" Width="11" Height="11">
                                <Border x:Name="checkBoxBackground" BorderBrush="{x:Null}"/>
                                <TextBlock x:Name="optionMark"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="13"
                                        Foreground="{telerik1:Office2016Resource ResourceKey=IconBrush}"
                                        FontFamily="{StaticResource TelerikWebUI}"
                                        FontWeight="Normal"
                                        FontStyle="Normal"
                                        Text="{StaticResource GlyphCheck}"
                                        Opacity="0"/>
                                <Border x:Name="indeterminateMark"
                                        Background="{telerik1:Office2016Resource ResourceKey=IconBrush}"
                                        CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Opacity="0"
                                        Width="9"
                                        Height="9"/>
