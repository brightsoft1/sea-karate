        <Setter Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlSelectionIndicator">
                    <Grid x:Name="GridRoot">
                        <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                            <Grid>
                                <Border Background="{TemplateBinding Background}" Opacity="0.25"/>
                                <Grid x:Name="LockPathGrid" Opacity="0" Width="36" Height="36">
                                    <Ellipse StrokeThickness="2" Stroke="{telerik1:Office2016Resource ResourceKey=IconBrush}"/>
                                    <TextBlock x:Name="glyph"
                                            Text="{StaticResource GlyphLock}"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Normal"
                                            FontStyle="Normal"
                                            HorizontalAlignment="Center"
                                            FontFamily="{StaticResource TelerikWebUI}"
                                            Foreground="{telerik1:Office2016Resource ResourceKey=IconBrush}"/>
                                </Grid>
                            </Grid>
                        </Border>
                        <Thumb x:Name="PART_TopResizer" Margin="0 -5 0 0" HorizontalAlignment="Stretch" VerticalAlignment="Top" Style="{StaticResource LayoutControlResizerStyle}" Cursor="SizeNS"/>
                        <Thumb x:Name="PART_LeftResizer" Margin="-5 0 0 0" HorizontalAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource LayoutControlResizerStyle}" Cursor="SizeWE"/>
                        <Thumb x:Name="PART_RightResizer"
                                Margin="0 0 -5 0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Stretch"
                                Style="{StaticResource LayoutControlResizerStyle}"
                                Cursor="SizeWE"/>
                        <Thumb x:Name="PART_BottomResizer"
                                Margin="0 0 0 -5"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource LayoutControlResizerStyle}"
                                Cursor="SizeNS"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsContextItemLocked" Value="True">
                            <Setter TargetName="LockPathGrid" Property="Opacity" Value="1"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
