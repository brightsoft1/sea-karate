                    <VisualState x:Name="Select">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="contentControl" Storyboard.TargetProperty="FontWeight">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <FontWeight>Bold</FontWeight>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="BorderVisual" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:Office2016Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="UnSelect"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="BorderVisual"
                    CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"/>
            <Grid Background="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Image
                        Style="{StaticResource BreadcrumbImageStyle}"
                        Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image}"
                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsIconVisible, Converter={StaticResource visibilityConverter}}"
                        Margin="-3 0 0 0"/>
                <ContentControl x:Name="contentControl"
                        Grid.Column="1"
                        FontSize="{TemplateBinding FontSize}"
                        Foreground="{TemplateBinding Foreground}"
                        Content="{TemplateBinding DropDownHeader}"
                        ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                        Margin="{TemplateBinding Padding}"
                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="FontFamily" Value="{telerik1:Office2016Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Office2016Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:Office2016Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MinWidth" Value="150"/>
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}"/>
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}"/>
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}"/>
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="15"/>
                        </Grid.ColumnDefinitions>
                        <Border x:Name="NormalVisual"
                                Grid.ColumnSpan="3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                        <Image x:Name="image"
                                Style="{StaticResource BreadcrumbImageStyle}"
                                Margin="3 0"
                                Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource visibilityConverter}}"/>
                        <telerik:RadSplitButton x:Name="historySplitButton"
                                IsTabStop="False"
                                Style="{StaticResource BreadcrumbTextSplitButton}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                TogglePartStyle="{StaticResource BreadCrumbHistoryToggleButtonStyle}"
                                IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}"
                                Grid.Column="1"
                                Grid.ColumnSpan="2">
                            <telerik:RadSplitButton.Content>
                                <TextBox x:Name="pathTextBox" BorderThickness="0" Margin="1" IsTabStop="False" VerticalAlignment="Center" Opacity="0" MinHeight="20" FontSize="{TemplateBinding FontSize}"/>
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox x:Name="HistoryListBox"
                                        Background="{telerik1:Office2016Resource ResourceKey=MainBrush}"
                                        BorderThickness="0"
                                        Foreground="{TemplateBinding Foreground}"
                                        ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                        ItemsSource="{TemplateBinding HistoryItems}"
                                        FontSize="{TemplateBinding FontSize}"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        Padding="1"
                                        SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}"
                                        MinHeight="26"/>
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <telerikBreadcrumb:RadBreadcrumbBar x:Name="BreadcrumbBar"
                                Grid.Column="1"
                                Background="Transparent"
                                Foreground="{TemplateBinding Foreground}"
                                FontSize="{TemplateBinding FontSize}"
                                ItemTemplate="{TemplateBinding ItemTemplate}"
                                ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"/>
                        <Border x:Name="FocusVisual"
                                Grid.ColumnSpan="3"
                                Background="{x:Null}"
                                BorderBrush="{telerik1:Office2016Resource ResourceKey=AccentFocusedBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"
                                Visibility="Collapsed"/>
                        <Popup x:Name="AutocompletePopup" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom" VerticalOffset="1">
                            <ListBox x:Name="AutoCompleteListBox"
                                    ItemsSource="{TemplateBinding AutocompleteItems}"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{TemplateBinding Foreground}"
                                    MinHeight="26"
                                    Padding="1"
                                    BorderThickness="1"
                                    MaxWidth="650"
                                    MaxHeight="500"
                                    MinWidth="150"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}"
                                    Background="{telerik1:Office2016Resource ResourceKey=PrimaryBrush}"
                                    BorderBrush="{telerik1:Office2016Resource ResourceKey=BasicBrush}"/>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsTextModeEnabled" Value="False">
                            <Setter TargetName="historySplitButton" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True"/>
                                <Condition Property="IsTextMode" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="pathTextBox" Property="Opacity" Value="1"/>
                            <Setter TargetName="NormalVisual" Property="Background" Value="{telerik1:Office2016Resource ResourceKey=MainBrush}"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True"/>
                                <Condition Property="IsTextMode" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Visible"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBar">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikBreadcrumb:RadBreadcrumbBar">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <ItemsPresenter x:Name="ItemsPresented"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarItemStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="3 3 3 0"/>
        <Setter Property="MinHeight" Value="26"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <Style TargetType="telerikNavigation:RadBreadcrumb" BasedOn="{StaticResource RadBreadcrumbStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBar" BasedOn="{StaticResource RadBreadcrumbBarStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBarItem" BasedOn="{StaticResource RadBreadcrumbBarItemStyle}"/>
    <Style x:Key="DesktopAlertCloseButtonStyle" TargetType="telerik:RadButton">
        <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="MinHeight" Value="24"/>
        <Setter Property="MinWidth" Value="24"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Bottom"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="CornerRadius" Value="{telerik1:Office2016Resource ResourceKey=CornerRadius}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadButton">
                    <Border x:Name="BorderVisual"
                            SnapsToDevicePixels="True"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{TemplateBinding Background}"
                            CornerRadius="{TemplateBinding CornerRadius}">
                        <TextBlock x:Name="Content"
                                Text="{StaticResource GlyphClose}"
                                FontSize="16"
                                FontStyle="Normal"
                                FontWeight="Normal"
                                FontFamily="{StaticResource TelerikWebUI}"
                                Foreground="{TemplateBinding Foreground}"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DesktopAlertDropDownButtonStyle" TargetType="telerik:RadDropDownButton">
        <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinHeight" Value="24"/>
        <Setter Property="MinWidth" Value="24"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="CornerRadius" Value="{telerik1:Office2016Resource ResourceKey=CornerRadius}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadDropDownButton">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                            <Grid VerticalAlignment="Center">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid x:Name="DropDownIndicator"
                                        IsHitTestVisible="False"
                                        Grid.Column="1"
                                        Grid.Row="2"
                                        HorizontalAlignment="Center"
                                        Canvas.ZIndex="2"
                                        VerticalAlignment="Center"
                                        RenderTransformOrigin="0.5,0.5"
                                        Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                    <TextBlock x:Name="arrow"
                                            Text="{StaticResource GlyphArrow60Down}"
                                            FontSize="16"
                                            FontStyle="Normal"
                                            FontWeight="Normal"
                                            FontFamily="{StaticResource TelerikWebUI}"
                                            Foreground="{TemplateBinding Foreground}"/>
                                </Grid>
                            </Grid>
                        </Border>
                        <Popup x:Name="DropDownPopup" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                            <Border x:Name="DropDownPopupBorder"
                                    BorderThickness="1"
                                    MinWidth="3"
                                    MinHeight="3"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadiusBottom}"
                                    Background="{telerik1:Office2016Resource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik1:Office2016Resource ResourceKey=BasicBrush}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                                <ContentControl x:Name="DropDownPopupContent"
                                        Content="{TemplateBinding DropDownContent}"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"
                                        ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"/>
                            </Border>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016Resource ResourceKey=AccentPressedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentPressedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="arrow" Property="Opacity" Value="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="RadDesktopAlertHeaderTemplate">
        <Grid x:Name="Header" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <StackPanel
                    Orientation="Horizontal"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Right"
                    DataContext="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadDesktopAlert}}">
                <telerik:RadDropDownButton x:Name="DesktopAlert_DropDownButton"
                        IsOpen="{Binding IsMenuOpen, Mode=TwoWay}"
                        Style="{StaticResource DesktopAlertDropDownButtonStyle}"
                        Visibility="{Binding ShowMenuButton, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <telerik:RadDropDownButton.DropDownContent>
                        <telerikNavigation:RadContextMenu ItemsSource="{Binding MenuItemsSource}" ItemContainerStyle="{Binding MenuItemContainerStyle}"/>
                    </telerik:RadDropDownButton.DropDownContent>
                </telerik:RadDropDownButton>
                <telerik:RadButton x:Name="DesktopAlert_CloseButton"
                        Command="{x:Static telerik:DesktopAlertCommands.Close}"
                        Style="{StaticResource DesktopAlertCloseButtonStyle}"
                        Visibility="{Binding ShowCloseButton, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <ToolTipService.ToolTip>
                        <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                    </ToolTipService.ToolTip>
                </telerik:RadButton>
            </StackPanel>
            <ContentPresenter
                    Content="{Binding}"
                    Grid.RowSpan="2"
                    VerticalAlignment="Center"
                    TextBlock.FontWeight="Bold"
                    MinHeight="{Binding ActualHeight, ElementName=DesktopAlert_CloseButton}"
                    Margin="0 2 42 0">
                <ContentPresenter.Resources>
                    <Style TargetType="TextBlock">
                        <Setter Property="TextWrapping" Value="Wrap"/>
                    </Style>
                </ContentPresenter.Resources>
            </ContentPresenter>
        </Grid>
    </DataTemplate>
    <ControlTemplate x:Key="RadDesktopAlertTemplate" TargetType="telerikNavigation:RadDesktopAlert">
        <Border x:Name="BorderVisual"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter
                        Content="{TemplateBinding Header}"
                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                        Grid.Row="0"
                        Grid.Column="1"
                        VerticalAlignment="Top"
                        TextBlock.Foreground="{TemplateBinding Foreground}"/>
                <ContentPresenter x:Name="Icon"
                        Grid.Column="0"
                        Grid.RowSpan="2"
                        Content="{TemplateBinding Icon}"
                        Width="{TemplateBinding IconColumnWidth}"
                        Margin="{TemplateBinding IconMargin}"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Center"
                        ContentTemplate="{TemplateBinding IconTemplate}"/>
                <ContentPresenter x:Name="Content"
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0 7 0 0"
                        TextBlock.Foreground="{TemplateBinding Foreground}"
                        telerikNavigation:WindowHost.HitTestable="True"
                        VerticalAlignment="Top"
                        HorizontalAlignment="Left"
                        Cursor="Hand">
                    <ContentPresenter.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="TextWrapping" Value="Wrap"/>
                            <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                            <Style.Triggers>
                                <Trigger Property="TextBlock.IsMouseOver" Value="True">
                                    <Setter Property="TextDecorations" Value="Underline"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </ContentPresenter.Resources>
                </ContentPresenter>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOverAlert" Value="True">
                <Setter Property="Background" Value="{telerik1:Office2016Resource ResourceKey=AccentPressedBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="DesktopAlertMenuItemContainerStyle" TargetType="telerikNavigation:RadMenuItem" BasedOn="{StaticResource RadMenuItemStyle}">
        <Setter Property="Header" Value="{Binding Header}"/>
        <Setter Property="Command" Value="{Binding Command}"/>
        <Setter Property="CommandParameter" Value="{Binding CommandParameter}"/>
        <Setter Property="ItemsSource" Value="{Binding ItemsSource}"/>
        <Setter Property="IsSeparator" Value="{Binding IsSeparator}"/>
        <Setter Property="IsCheckable" Value="{Binding IsCheckable}"/>
        <Setter Property="IsChecked" Value="{Binding IsChecked}"/>
        <Setter Property="Icon" Value="{Binding IconUrl}"/>
        <Setter Property="IconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Image Source="{Binding}" Stretch="None"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadDesktopAlertStyle" TargetType="telerikNavigation:RadDesktopAlert">
        <Setter Property="FontFamily" Value="{telerik1:Office2016Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Office2016Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="Background" Value="{telerik1:Office2016Resource ResourceKey=AccentBrush}"/>
        <Setter Property="Padding" Value="9 4 4 8"/>
        <Setter Property="Opacity" Value="1"/>
        <Setter Property="Width" Value="360"/>
        <Setter Property="Height" Value="100"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="HeaderTemplate" Value="{StaticResource RadDesktopAlertHeaderTemplate}"/>
        <Setter Property="MenuItemContainerStyle" Value="{StaticResource DesktopAlertMenuItemContainerStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadDesktopAlertTemplate}"/>
    </Style>
    <Style TargetType="telerikNavigation:RadDesktopAlert" BasedOn="{StaticResource RadDesktopAlertStyle}"/>
</ResourceDictionary>
