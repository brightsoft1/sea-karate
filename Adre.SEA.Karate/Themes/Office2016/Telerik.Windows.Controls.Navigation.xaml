    <Style x:Key="RadialMenuButtonStyle" TargetType="telerikRadialMenu:RadialMenuButton">
        <Setter Property="FontFamily" Value="{telerik1:Office2016Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Office2016Resource ResourceKey=FontSize}"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="Foreground" Value="{telerik:Office2016Resource ResourceKey=IconBrush}"/>
        <Setter Property="Background" Value="{telerik:Office2016Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2016Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="MinWidth" Value="0"/>
        <Setter Property="Width" Value="48"/>
        <Setter Property="Height" Value="48"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock
                            Text="{StaticResource GlyphGear}"
                            FontSize="32"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontWeight="Normal"
                            FontStyle="Normal"
                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="BackContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock
                            Text="{StaticResource GlyphArrowLeft}"
                            FontSize="32"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontWeight="Normal"
                            FontStyle="Normal"
                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                            Margin="0 4 0 0"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:RadialMenuButton">
                    <Grid x:Name="RootGrid">
                        <Grid>
                            <Ellipse x:Name="BackgroundGlyph"
                                    Fill="{TemplateBinding Background}"
                                    Stroke="{TemplateBinding BorderBrush}"
                                    StrokeThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness.Left}"
                                    RenderTransformOrigin=".5,.5"
                                    Width="{TemplateBinding Width}"
                                    Height="{TemplateBinding Height}"/>
                            <ContentPresenter x:Name="NormalGlyph"
                                    RenderTransformOrigin="0.5,0.5"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding Content}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                    Margin="0 -1 0 0"/>
                            <ContentPresenter x:Name="BackGlyph"
                                    HorizontalAlignment="Center"
                                    Margin="0 -3 0 0"
                                    ContentTemplate="{Binding Path=BackContentTemplate, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                    VerticalAlignment="Center"
                                    RenderTransformOrigin="0.5,0.5"
                                    Visibility="Collapsed"/>
                        </Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="NavigationStates">
                                <VisualState x:Name="Back">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Home">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NormalGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BackGlyph" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="{telerik:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik:Office2016Resource ResourceKey=AccentPressedBrush}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Stroke" Value="{telerik:Office2016Resource ResourceKey=AccentPressedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="NormalGlyph" Property="Opacity" Value="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="BackGlyph" Property="Opacity" Value="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="BackgroundGlyph" Property="Fill" Value="{telerik1:Office2016Resource ResourceKey=PrimaryBrush}"/>
                        </Trigger>
                        <Trigger Property="FlowDirection" Value="RightToLeft">
                            <Setter Property="BackContentTemplate">
                                <Setter.Value>
                                    <DataTemplate>
                                        <TextBlock
                                                Text="{StaticResource GlyphArrowRight}"
                                                FontSize="32"
                                                FontFamily="{StaticResource TelerikWebUI}"
                                                FontWeight="Normal"
                                                FontStyle="Normal"
                                                Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerikRadialMenu:RadialMenuButton}}}"
                                                Margin="0 4 0 0"/>
                                    </DataTemplate>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NavigationItemButtonStyle" TargetType="telerikRadialMenu:NavigationItemButton">
        <Setter Property="Foreground" Value="{telerik:Office2016Resource ResourceKey=IconBrush}"/>
        <Setter Property="Background" Value="{telerik:Office2016Resource ResourceKey=ComplementaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2016Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRadialMenu:NavigationItemButton">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_PathElement" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_ArrowGlyph" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_PathElement" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Office2016Resource ResourceKey=PrimaryBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_ArrowGlyph" Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik:Office2016Resource ResourceKey=MarkerBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation
                                                Duration="0:0:0.3"
                                                From="0"
                                                To="{telerik1:Office2016Resource ResourceKey=DisabledOpacity}"
                                                Storyboard.TargetProperty="Opacity"
                                                Storyboard.TargetName="PART_ArrowGlyph"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Path x:Name="PART_ArrowGlyph"
                                IsHitTestVisible="False"
                                Fill="{TemplateBinding Foreground}"
                                Stretch="Fill"
                                Width="8"
                                Height="5"
                                Data="{StaticResource ThinArrowTop8x5}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Canvas.ZIndex="102"/>
                        <Path x:Name="PART_PathElement"
                                Fill="{TemplateBinding Background}"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"
                                Canvas.ZIndex="100"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
