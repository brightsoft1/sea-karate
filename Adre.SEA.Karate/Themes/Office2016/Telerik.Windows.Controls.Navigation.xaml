                                <Setter TargetName="HeaderPart" Property="Background" Value="{telerik1:Office2016Resource ResourceKey=PrimaryBrush}"/>
                                <Setter TargetName="HeaderPart" Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"/>
                                <Setter TargetName="HeaderPart" Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentPressedBrush}"/>
                                <Setter TargetName="outerContainer" Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentPressedBrush}"/>
                            </MultiTrigger.Setters>
                        </MultiTrigger>
