            <Setter.Value>
                <ControlTemplate>
                    <Grid HorizontalAlignment="Left" Width="14">
                        <Border Background="Transparent" HorizontalAlignment="Right" VerticalAlignment="Center" CornerRadius="{telerik1:Office2016Resource ResourceKey=CornerRadius}"/>
                        <AdornedElementPlaceholder x:Name="Holder"/>
