                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="13"
                                        Foreground="{telerik1:Office2016Resource ResourceKey=IconBrush}"
                                        FontFamily="{StaticResource TelerikWebUI}"
                                        FontWeight="Normal"
                                        FontStyle="Normal"
                                        Text="{StaticResource GlyphCheck}"
                                        Opacity="0"/>
