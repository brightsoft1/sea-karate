                        <telerik:RadButton x:Name="input7_Button" Content="7" Grid.Row="2" Grid.Column="0" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" FontWeight="Bold" CommandParameter="7"/>
                        <telerik:RadButton x:Name="input8_Button" Content="8" Grid.Row="2" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" FontWeight="Bold" CommandParameter="8"/>
                        <telerik:RadButton x:Name="input9_Button" Content="9" Grid.Row="2" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" FontWeight="Bold" CommandParameter="9"/>
                        <telerik:RadButton x:Name="Divide_Button"
                                Content="/"
                                Grid.Row="2"
                                Grid.Column="3"
