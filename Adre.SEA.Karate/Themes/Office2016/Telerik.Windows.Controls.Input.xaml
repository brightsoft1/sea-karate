                        <telerik:RadButton x:Name="Ms_Button" Content="MS" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.MemoryStore}" Style="{StaticResource CalculatorMemoryButtonStyle}"/>
                        <telerik:RadButton x:Name="Mplus_Button"
                                Content="M+"
                                Grid.Column="3"
                                Command="{x:Static controls:RadCalculatorCommands.MemoryPlus}"
                                Style="{StaticResource CalculatorMemoryButtonStyle}"/>
                        <telerik:RadButton x:Name="Mminus_Button"
                                Content="M-"
                                Grid.Column="4"
                                Command="{x:Static controls:RadCalculatorCommands.MemoryMinus}"
                                Style="{StaticResource CalculatorMemoryButtonStyle}"/>
                        <telerik:RadButton x:Name="Delete_Button" Content="←" Grid.Row="1" Grid.Column="0" Command="{x:Static controls:RadCalculatorCommands.Delete}"/>
                        <telerik:RadButton x:Name="ClearInput_Button" Content="CE" Grid.Row="1" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.ClearInput}"/>
                        <telerik:RadButton x:Name="ClearEverything_Button" Content="C" Grid.Row="1" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.ClearEverything}"/>
                        <telerik:RadButton x:Name="Negate_Button"
